{
  "name": "browserify-plugin-can-compile",
  "version": "1.2.0",
  "description": "This Browserify plugin compiles CanJs Templates into an extra Javascript file.",
  "main": "index.js",
  "author": "SCENECS <scenecs@t-online.de>",
  "license": "MIT",
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/scenecs/browserify-plugin-can-compile"
  },
  "bugs": {
    "url": "https://github.com/scenecs/browserify-plugin-can-compile/issues"
  },
  "keywords": [
    "browserify",
    "plugin",
    "canjs",
    "stache",
    "compiler"
  ],
  "directories": {
    "doc": "./doc/",
    "test": "./test/"
  },
  "engines": {
    "node" : ">=4.3"
  },
  "scripts": {
    "precompile": "npm run clean",
    "compile": "$(npm bin)/babel --presets es2015 -d ./ ./src",
    "watch": "node -e \"require('fs').watch('./src/', {\"recursive\": true }, function() { var process = require('child_process').spawn('npm', ['run', 'compile']); process.stdout.on('data', function(data) { console.log(data.toString()); }); })\"",
    "prepublish": "npm run compile",
    "clean": "node -e \"require('del').sync(['./**/*', '!.*', '!./src', '!./src/**/*', '!./test', '!./test/**/*', '!./node_modules', '!./node_modules/**/*', '!./package.json', '!./canjsBinaryCache', '!./canjsBinaryCache/**/*', '!./*.md']);\"",
    "test": "$(npm bin)/istanbul cover $(npm bin)/_mocha -- -c --recursive --require babel-core/register",
    "test-watch": "$(npm bin)/mocha --require babel-core/register -w -c --recursive"
  },
  "dependencies": {
    "semver": "~5.3.0",
    "mkdirp": "~0.5.1",
    "extend": "~3.0.0",
    "through2": "~2.0.1",
    "can-compile": "~0.10.2",
    "es6-promise": "~3.2.1"
  },
  "devDependencies": {
    "babel-cli": "~6.14.0",
    "babel-preset-es2015": "~6.14.0",
    "del": "~2.2.2",
    "instanbul": "gotwarlost/istanbul#v1.1.0-alpha.1",
    "chai": "~3.5.0",
    "mocha": "~3.0.2",
    "browserify": "~13.1.0"
  }
}
